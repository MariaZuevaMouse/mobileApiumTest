<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Native mobile testing Suite">
    <parameter name="appType" value="native"></parameter>
<!--    <parameter name="deviceName" value="GOOGLE Pixel 2"></parameter>-->
    <parameter name="UDID" value="HT7AD1A00541"></parameter>
<!--    <parameter name="udid" value="89HY075AQ"></parameter>-->
    <parameter name="platformName" value="Android"></parameter>
    <parameter name="appPackage" value="platovsky.alexey.epamtestapp"></parameter>
    <parameter name="appActivity" value="activities.LoginActivity"></parameter>
    <test name="Native tests">
        <groups>
            <run>
                <include name="native"/>
            </run>
        </groups>
        <classes>
            <class name="scenarios.nativeMobileTests"></class>
        </classes>
    </test>
</suite>